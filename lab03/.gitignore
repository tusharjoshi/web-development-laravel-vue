# .gitignore - list of file path patterns not to be tracked by source control

# NodeJS dependencies can be downloaded
/node_modules

# temporary file used by dev server
/public/hot

# local symlink created by php artisan storage:link
/public/storage

# secret encryption keys for services like OAuth
# created by php artisan passport:keys
/storage/*.key

# composer packages, they can be downloaded again
/vendor

# Local variables are specific to local system only
.env
.env.backup

# phpunit generated files
.phpunit.result.cache
.phpunit.cache

# local debug files
npm-debug.log
yarn-error.log

# IDE specific local configurations
/.fleet
/.idea

# adds .vscode folder to ignored files
.vscode/**/*

# exclude these files from being ignored
!.vscode/settings.json
!.vscode/extensions.json
!.vscode/launch.json
!.vscode/tasks.json

# macOS folder configuration attributes
*DS_Store

# generated files
/public/build
